{
  "name": "voskobovich/yii2-api-toolkit",
  "description": "New level create API on Yii 2 Framework. Speed, security, flexibility!",
  "keywords": [
    "crud",
    "rest",
    "api",
    "yii2"
  ],
  "homepage": "https://github.com/voskobovich/yii2-api-toolkit",
  "type": "yii2-toolkit",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/voskobovich/yii2-api-toolkit/issues",
    "source": "https://github.com/voskobovich/yii2-api-toolkit"
  },
  "authors": [
    {
      "name": "Vitaly Voskobovich",
      "email": "vitaly@voskobovich.com",
      "homepage": "http://voskobovich.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.0",
    "tuyakhov/yii2-json-api": "^0.0.11"
  },
  "require-dev": {
    "yiisoft/yii2-coding-standards": "~2.0.2",
    "friendsofphp/php-cs-fixer": "^2"
  },
  "autoload": {
    "psr-4": {
      "voskobovich\\api\\controllers\\": "src/controllers",
      "voskobovich\\api\\actions\\": "src/actions",
      "voskobovich\\api\\forms\\": "src/forms",
      "voskobovich\\api\\filters\\": "src/filters",
      "voskobovich\\api\\filters\\auth\\": "src/filters/auth"
    }
  }
}
